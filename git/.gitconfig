[core]
	editor = vim
	askpass = 
	compression = 9
	preloadindex = true
	fscache = true
[mergetool "vimdiff"]
  cmd = nvim -d $LOCAL $REMOTE $MERGED -c '$wincmd w' -c 'wincmd J'
[gc]
	auto = 256
[diff]
	tool = vimdiff
