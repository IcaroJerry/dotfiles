	tool = vimdiff
[difftool]
	prompt = false
[pack]
	windowMemory = 100m
	packSizeLimit = 100m
[http]
	postBuffer = 1048576000
[color]
	ui = true
[alias]
	d = difftool
[push]
	default = simple
[rerere]
	enabled = true
[cola]
	spellcheck = false
